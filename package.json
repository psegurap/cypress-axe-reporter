    "description": "Simple reporter that integrates easily with Cypress and Cypress-Axe to output accessibility violations in a HTML format.",
    "engines": {
        "node": ">=14"
    },
    "main": "index.js",
    "scripts": {
        "build-styles": "npx tailwindcss -i ./tailwind/styles.css -o ./lib/styles.min.css --watch --minify"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/psegurap/cypress-axe-reporter.git"
    },
    "keywords": [
        "cypress",
        "axe-core",
        "cypress-axe"
    ],
    "author": "Pedro Segura",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/psegurap/cypress-axe-reporter/issues"
    },
    "homepage": "https://github.com/psegurap/cypress-axe-reporter#readme",
    "dependencies": {
        "fs-extra": "^11.2.0",
        "jsdom": "^25.0.1"
    },
    "peerDependencies": {
        "axe-core": "^3 || ^4",
        "cypress": "^10 || ^11 || ^12 || ^13 || ^14 || ^15",
        "cypress-axe": "^1"
    },
    "devDependencies": {
        "tailwindcss": "^3.4.18"
    }
